# QuietInbox Backend Environment Variables
# Copy this file to .env and update with your values

# Database
POSTGRES_USER=quietinbox
POSTGRES_PASSWORD=quietinbox
POSTGRES_DB=quietinbox
DATABASE_URL=postgresql://quietinbox:quietinbox@postgres:5432/quietinbox

# Redis
REDIS_URL=redis://redis:6379

# JWT Secret (change in production!)
SECRET_KEY=your-super-secret-key-change-this-in-production

# API Settings
API_HOST=0.0.0.0
API_PORT=8000
